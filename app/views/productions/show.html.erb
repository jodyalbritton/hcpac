<div class="page-header">
  <%= link_to productions_path, :class => 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_production_path(@production), :class => 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show production</h1>
</div>

<dl class="dl-horizontal">
  <dt>Title:</dt>
  <dd><%= @production.title %></dd>

  <dt>Description:</dt>
  <dd><%= @production.description %></dd>

  <dt>Start date:</dt>
  <dd><%= @production.start_date %></dd>

  <dt>End date:</dt>
  <dd><%= @production.end_date %></dd>

</dl>
